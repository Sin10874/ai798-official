{
  "version": 2,
  "builds": [
    {
      "src": "index.html",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/favicon\\.(svg|ico|png)",
      "dest": "/favicon.$1"
    },
    {
      "src": "/site\\.webmanifest",
      "dest": "/site.webmanifest"
    },
    {
      "src": "/(.*\\.(png|jpg|jpeg|gif|svg|webp|ico))",
      "dest": "/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}

