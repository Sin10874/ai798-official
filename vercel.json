{
  "version": 2,
  "builds": [
    {
      "src": "index.html",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/favicon\\.(svg|ico|png)",
      "dest": "/favicon.$1"
    },
    {
      "src": "/site\\.webmanifest",
      "dest": "/site.webmanifest"
    },
    {
      "src": "/particles/(.*)",
      "dest": "/particles/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "cleanUrls": true,
  "trailingSlash": false,
  "redirects": [
    {
      "source": "/course-calendar",
      "destination": "/14daycoursecalendar.html",
      "permanent": false
    },
    {
      "source": "/14d-course-calendar",
      "destination": "/14daycoursecalendar.html",
      "permanent": false
    },
    {
      "source": "/particles",
      "destination": "/particles/index.html",
      "permanent": false
    }
  ]
}

